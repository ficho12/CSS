	tst = function () {
		var _ = this;
		var tstOpen = false;		
        var HeightUl;
        $tst = document.querySelector(".tst");  
		$tst_li = document.querySelector(".tst .tst-li");
		$tst_li_all = document.querySelectorAll(".tst-li li");
		$btn_tst = document.querySelector(".btn-tst");

		_.semTstResize = function() {			  
	        var sumLi = 0;
			var WidthUl = $tst_li.clientWidth;	        
			$tst_li_all.forEach( function(item) { sumLi += item.offsetWidth; });
            HeightUl = $tst_li.clientHeight
			if ( sumLi < WidthUl ){
				$btn_tst.style.display = "none";
				if( $tst.classList.contains("tst--open") ) {
					$tst.classList.remove("tst--open"); 
				} 
	    	} else { 
					$btn_tst.style.display = "";
			}
        }
    	_.semTstResize();

    	$btn_tst.addEventListener("click", function(){
    		$tst.classList.toggle("tst--open");
			if ( tstOpen == false){
				$tst.style.height = HeightUl + "px";
				tstOpen = true;
			} else{
				$tst.style.height = "";
				tstOpen = false;
			}
    	});
 		window.addEventListener("resize", _.semTstResize);	 
	}();
});

