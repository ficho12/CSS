<script>
  
  var el = document.querySelector(".mod-i-comments .btn-comentario");
  function handler(e) { 
    e.preventDefault();
    el.classList.toggle('folded');
    document.querySelector(".mod-i-comments").classList.toggle("folded"); 
  } 
  
  function toggle(e) { 
    e.preventDefault();
    this.classList.toggle("folded");
    document.querySelector(".mod-i-comments").classList.toggle("folded");    
   } 

  function loadEnlaceComentarios() {
    el.classList.toggle("folded");
    document.querySelector(".mod-i-comments").classList.toggle("folded");
    setTimeout(function(){ location.href = "#EnlaceComentarios"; }, 500);
  }

  el.addEventListener("click", handler, false);

  var enlace_comentarios = document.querySelectorAll("a[href='#EnlaceComentarios']");
  [].forEach.call(enlace_comentarios,function(e){
      e.addEventListener('click',function(e){
      e.preventDefault();
      loadEnlaceComentarios();
    },false)
  })

  if(window.location.hash == "#EnlaceComentarios"){
    loadEnlaceComentarios();  
  }

</script>
