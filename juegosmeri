<script type="text/javascript">
window.addEventListener("scroll", function(){
	var r = document.getElementById('outbrain_widget').getBoundingClientRect();
	var outbrain_widget_position = r.top - 500;
	//console.log("outbrain: "+outbrain_widget_position+" scroll: "+window.scrollY);
	if(window.scrollY >= outbrain_widget_position){
		document.getElementById('outbrain_widget_script').setAttribute('src', document.getElementById('outbrain_widget_script').getAttribute('data-src'));
		document.getElementById('outbrain_widget_script').removeAttribute('data-src');
	}
});
</script>
